{
    "name": "tm/gpg-verifier",
    "description": "Simple library to verify a file with gpg signature.",
    "keywords": [
        "gpg",
        "verifier",
        "verification",
        "signature"
    ],
    "homepage": "https://github.com/tommy-muehle/php-gpg-verifier",
    "authors": [
        {
            "name": "Tommy Muehle",
            "email": "tommy.muehle@gmail.com",
            "homepage": "https://tommy-muehle.de"
        }
    ],
    "support": {
        "email": "tommy.muehle@gmail.com",
        "issues": "https://github.com/tommy-muehle/php-gpg-verifier/issues",
        "docs": "https://github.com/tommy-muehle/php-gpg-verifier/blob/master/README.md",
        "source": "https://github.com/tommy-muehle/php-gpg-verifier/tree/master"
    },
    "license": "MIT",
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "php-mock/php-mock-phpunit": "1.1.*",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "TM\\GPG\\Verification\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TM\\GPG\\Verification\\": "src/",
            "TM\\GPG\\Verification\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
